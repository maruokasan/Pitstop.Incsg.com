<!DOCTYPE html>
<html lang="en">
<head>
    <base href=""/>
    <meta charset="utf-8" />
    <title>Pitstop.Incsg</title>
    <meta property="og:url" content="https://keenthemes.com/metronic" />
    <meta property="og:site_name" content="Keenthemes | Metronic" />
    <link rel="canonical" href="https://preview.keenthemes.com/metronic8" />
    <link rel="shortcut icon" href="~/images/pitstoplogo1.png" />
    <!--begin::Fonts-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" />
    <!--end::Fonts-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta content="utf-8" http-equiv="encoding" />
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Include jQuery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <!--begin::Global Stylesheets Bundle(mandatory for all pages)-->
    <link href="~/assets/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/plugins/custom/prismjs/prismjs.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Global Stylesheets Bundle-->
    <!-- Add SweetAlert CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/11.0.18/sweetalert2.min.css">
</head>
<body>
<!-- Content -->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    @RenderBody()
</div>

<div class="d-flex justify-content-between">
    <button type="button" class="back-btn btn btn-light-primary font-weight-bolder text-uppercase px-9 py-4 mt-10 mb-10" onclick="history.back()" data-wizard-type="action-prev" style="margin-left: 40px;">Back</button>
</div>

<!-- Footer -->
<div class="footer py-4 d-flex flex-lg-column" id="kt_footer">
    <!-- Container -->
    <div class="container-xxl d-flex flex-column flex-md-row flex-stack">
        <!-- Copyright -->
        <div class="text-gray-900 order-2 order-md-1">
            <span class="text-gray-500 fw-semibold me-1">Created by</span>
            <a href="https://keenthemes.com" target="_blank" class="text-muted text-hover-primary fw-semibold me-2 fs-6">Pitstop.Incsg</a>
        </div>
    </div>
    <!--end::Container-->
</div>
<!--end::Footer-->

<!-- Scroll Up -->
<a href="#" class="scrollup" id="scroll-up"> 
    <i class='bx bx-up-arrow-alt scrollup__icon'></i>
</a>

@await RenderSectionAsync("Scripts", required: false)

<!--begin::Global Javascript Bundle-->
<script src="~/assets/plugins/global/plugins.bundle.js"></script>
<script src="~/assets/js/scripts.bundle.js"></script>
<!--end::Global Javascript Bundle-->
<!-- Add SweetAlert JavaScript -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/11.0.18/sweetalert2.min.js"></script>

<script>
    $(document).ajaxStart(function () {
        $('.custom-loader-wrapper').css('display', 'flex');
    }).ajaxStop(function () {
        $('.custom-loader-wrapper').css('display', 'none');
    });

    function getRedirect(url) {
        $.ajax({
            url: contentUrl + "Home/CreateToken",
            type: "GET",
            dataType: "JSON",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            success: function (result) {
                if (result != null) {
                    window.open(url + result, '_blank');
                }
            },
            error: function (request, status, error) {
                // Use Swal.fire for displaying SweetAlert
                Swal.fire({
                    icon: 'error',
                    title: request.responseJSON.message,
                    showConfirmButton: true
                });
            }
        });
    }
</script>

</body>
</html>
